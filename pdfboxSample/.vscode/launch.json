	"configurations": [
		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/build/pdfboxTester",
			"args": [],
			// "args": [
			// 	"-s",
			// 	"/samples/sample01.pdf",
			// 	"-r",
			// 	"/samples/result"
			// ],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/build",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "gdb에 자동 서식 지정 사용",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"justMyCode": false
		}
	]
}